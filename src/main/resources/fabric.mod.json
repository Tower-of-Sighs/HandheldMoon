{
  "schemaVersion": 1,
  "id": "handheldmoon",
  "version": "${version}",
  "name": "HandheldMoon",
  "description": "This mod adds several large-area lighting items such as flashlights and mini moons.",
  "authors": [
    "MerrySnow",
    "Flechazo"
  ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/handheld-moon",
    "sources": "https://github.com/Tower-of-Sighs/HandheldMoon",
    "issues": "https://github.com/Tower-of-Sighs/HandheldMoon/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/handheldmoon/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.sighs.handheldmoon.client.HandheldMoonClient"
    ],
    "main": [
      "com.sighs.handheldmoon.HandheldMoon"
    ],
    "dynamiclights": [
      "com.sighs.handheldmoon.lights.HandheldMoonDynamicLightsInitializer"
    ],
    "modmenu": [
      "com.sighs.handheldmoon.compat.modmenu.ModMenuIntegration"
    ]
  },
  "mixins": [
    "handheldmoon.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "lambdynlights": "*",
    "forgeconfigapiport": "*"
  },
  "accessWidener": "handheldmoon.accesswidener",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/handheld-moon",
        "modmenu.discord": "https://discord.gg/C8epbbwjrS",
        "modmenu.wiki": "https://doc.sighs.cc/HandheldMoon/intro"
      }
    }
  }
}
