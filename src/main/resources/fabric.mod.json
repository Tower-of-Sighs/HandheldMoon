{
  "schemaVersion": 1,
  "id": "handheldmoon",
  "version": "${version}",
  "name": "HandheldMoon",
  "description": "Loot Bags is a mod which adds bags that drop other items, renewed by src_resources.",
  "authors": [
    "MerrySnow",
    "Flechazo"
  ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/handheldmoon/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.sighs.handheldmoon.client.HandheldMoonClient"
    ],
    "main": [
      "com.sighs.handheldmoon.HandheldMoon"
    ],
    "lambdynlights:initializer": [
      "com.sighs.handheldmoon.lights.HandheldMoonDynamicLightsInitializer"
    ]
  },
  "mixins": [
    "handheldmoon.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "lambdynlights": "*",
    "forgeconfigapiport": "*"
